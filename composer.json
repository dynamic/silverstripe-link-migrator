{
    "name": "dynamic/silverstripe-link-migrator",
    "description": "Migrate Linkable Link records to SilverStripe Link",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "linkable",
        "link",
        "migrate"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "gorriecoe/silverstripe-link": "^1.2",
        "sheadawson/silverstripe-linkable": "^2.0",
        "silverstripe/framework": "^4.0",
        "silverstripe/admin": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Dynamic\\Link\\Task\\": "tasks/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
